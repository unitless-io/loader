{
  "name": "@unitless-io/loader",
  "version": "0.0.2",
  "description": "Unitless Loader",
  "files": [
    "dist"
  ],
  "main": "dist/loader.js",
  "types": "dist/loader.d.ts",
  "scripts": {
    "build": "npm run build-loader && npm run build-web",
    "build-loader": "tsc --project tsconfig.loader.json",
    "build-web": "webpack",
    "lint": "eslint --ext ts src && npm run prettier-check",
    "prettier-check": "prettier --ignore-path .gitignore --check \"./**/*.ts\" \"./**/*.json\"",
    "test": "jest",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unitless-io/loader.git"
  },
  "author": "The Unitless Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unitless-io/loader/issues"
  },
  "homepage": "https://unitless.io",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.11",
    "@types/node-fetch": "^2.6.1",
    "@types/serialize-javascript": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "buffer": "^6.0.3",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "serialize-javascript": "^6.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
}
