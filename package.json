    "ts-loader": "^9.3.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@unitless-io/data-server": "1.0.1",
